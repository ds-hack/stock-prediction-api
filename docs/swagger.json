{
    "swagger": "2.0",
    "info": {
        "description": "Demonstration api for dshack project",
        "title": "Stock Prediction API",
        "contact": {
            "name": "Contact us",
            "url": "https://datascientist-toolbox.com/",
            "email": "datascientist.toolbox.com@gmail.com"
        },
        "license": {
            "name": "Apache 2.0",
            "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
        },
        "version": "1.0"
    },
    "host": "localhost:30080",
    "basePath": "/api/v1",
    "paths": {
        "/companies": {
            "get": {
                "description": "銘柄コードで指定した特定の企業についての詳細情報を取得する",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "companies"
                ],
                "summary": "銘柄コードで指定した特定の企業情報を取得",
                "parameters": [
                    {
                        "maxLength": 16,
                        "minLength": 4,
                        "type": "string",
                        "description": "銘柄コード",
                        "name": "stockCode",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/models.CompanyWrapper"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/models.HTTPError"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/models.HTTPError"
                        }
                    }
                }
            }
        },
        "/stockprices/ma/{stockCode}": {
            "get": {
                "description": "指定の企業について、指定の計算方法による移動平均株価を取得する",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "stockprices"
                ],
                "summary": "移動平均株価データを取得",
                "parameters": [
                    {
                        "maxLength": 16,
                        "minLength": 4,
                        "type": "string",
                        "description": "銘柄コード",
                        "name": "stockCode",
                        "in": "path",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "移動平均の計算方法 + 計算日数のセットをカンマ区切りで指定 ex) sma5,ema25",
                        "name": "maTypes",
                        "in": "query",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "株価データ取得の開始日 YYYY-MM-DDの形式で指定",
                        "name": "startDate",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "株価データ取得の終了日 YYYY-MM-DDの形式で指定",
                        "name": "endDate",
                        "in": "query"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/models.RawStockPriceWrapper"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/models.HTTPError"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/models.HTTPError"
                        }
                    }
                }
            }
        },
        "/stockprices/rawdata/{stockCode}": {
            "get": {
                "description": "指定の企業について、株価の生データ(open/high/low/close/volume)を取得する",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "stockprices"
                ],
                "summary": "株価の生データを取得",
                "parameters": [
                    {
                        "maxLength": 16,
                        "minLength": 4,
                        "type": "string",
                        "description": "銘柄コード",
                        "name": "stockCode",
                        "in": "path",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "株価データ取得の開始日 YYYY-MM-DDの形式で指定",
                        "name": "startDate",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "株価データ取得の終了日 YYYY-MM-DDの形式で指定",
                        "name": "endDate",
                        "in": "query"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/models.RawStockPriceWrapper"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/models.HTTPError"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/models.HTTPError"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "models.Company": {
            "type": "object",
            "properties": {
                "companyId": {
                    "description": "システム内で設定する企業毎に一意となるコード",
                    "type": "string"
                },
                "companyName": {
                    "description": "企業名",
                    "type": "string"
                },
                "countryCode": {
                    "description": "国コード",
                    "type": "string"
                },
                "foundationDate": {
                    "description": "設立日(月)",
                    "type": "string"
                },
                "latitude": {
                    "description": "本社所在地緯度",
                    "type": "number"
                },
                "listedMarket": {
                    "description": "上場市場",
                    "type": "string"
                },
                "longitude": {
                    "description": "本社所在地経度",
                    "type": "number"
                },
                "stockCode": {
                    "description": "銘柄コード",
                    "type": "string"
                }
            }
        },
        "models.CompanyWrapper": {
            "type": "object",
            "properties": {
                "companies": {
                    "description": "企業名・銘柄コード・設立日などの会社の基本情報を保持するクラスの配列",
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/models.Company"
                    }
                },
                "timestamp": {
                    "description": "クライアントへレスポンスを送信する日時",
                    "type": "string"
                }
            }
        },
        "models.HTTPError": {
            "type": "object",
            "properties": {
                "message": {
                    "description": "エラーに対する付加的な情報",
                    "type": "string"
                },
                "status": {
                    "description": "HTTP Status Code",
                    "type": "integer"
                }
            }
        },
        "models.RawStockPrice": {
            "type": "object",
            "properties": {
                "close": {
                    "description": "株価(終値)",
                    "type": "number"
                },
                "date": {
                    "description": "日付",
                    "type": "string"
                },
                "high": {
                    "description": "株価(高値)",
                    "type": "number"
                },
                "low": {
                    "description": "株価(安値)",
                    "type": "number"
                },
                "open": {
                    "description": "株価(始値)",
                    "type": "number"
                },
                "volume": {
                    "description": "出来高",
                    "type": "number"
                }
            }
        },
        "models.RawStockPriceWrapper": {
            "type": "object",
            "properties": {
                "companyId": {
                    "description": "システム内で設定する企業毎に一意となるコード",
                    "type": "string"
                },
                "companyName": {
                    "description": "企業名",
                    "type": "string"
                },
                "data": {
                    "description": "株価の時系列データ本体",
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/models.RawStockPrice"
                    }
                },
                "stockCode": {
                    "description": "銘柄コード",
                    "type": "string"
                },
                "timestamp": {
                    "description": "クライアントへレスポンスを送信する日時",
                    "type": "string"
                }
            }
        }
    }
}